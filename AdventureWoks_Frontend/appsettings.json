{
  "ConnectionStrings": {
    "Redis": "localhost:6379,connectTimeout=1000, syncTimeout=1000, abortConnect=false"
  },
  "AllowedHosts": "*",
  "AdventureWorksApiBaseUrl": "https://localhost:7055",
  "BackendCredentials": {
    "Username": "user1",
    "Password": "password1"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "ApplicationName": "AdventureWorks Frontend"
    }
  }
}
